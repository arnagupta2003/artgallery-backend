{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "start:server": "node ./dist/src/index.js",
    "start:worker": "node ./dist/src/index-worker.js",
    "codegen": "graphql-codegen --config codegen.ts",
    "build:admin": "rimraf admin-ui && ts-node src/compile-admin-ui.ts && npm run replace-i18n",
    "copy": "copyfiles -u 1 \"admin-ui/**/*\" dist/src/admin-ui",
    "replace-i18n": "copyfiles -f src/en.json admin-ui/dist/browser/i18n-messages",
    "build": "tsc && cpx \"src/plugins/**/ui/**/*\" dist/src/plugins && cpx \"static/**/*\" dist/static && npm run copy",
    "start": "concurrently npm:start:*",
    "deploy": "npm run build && npm run compile-admin && npm run start"
  },
  "dependencies": {
    "@angular/cli": "^20.0.2",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/typescript": "4.1.6",
    "@nestjs/graphql": "^13.0.4",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.0",
    "@vendure/admin-ui-plugin": "^3.2.2",
    "@vendure/asset-server-plugin": "3.2.2",
    "@vendure/cli": "^3.2.2",
    "@vendure/core": "^3.3.4",
    "@vendure/elasticsearch-plugin": "^3.2.3",
    "@vendure/email-plugin": "^3.3.4",
    "@vendure/ui-devkit": "^3.2.2",
    "concurrently": "9.1.2",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "dotenv": "16.4.7",
    "html2pdf.js": "^0.10.3",
    "mkdirp": "^3.0.1",
    "pg": "^8.16.0",
    "pg-copy-streams": "^7.0.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  }
}
